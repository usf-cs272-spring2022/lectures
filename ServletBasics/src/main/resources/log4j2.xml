<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE log4j:configuration>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%thread: %message%n">
			</PatternLayout>
		</Console>
	</Appenders>

	<Loggers>
		<Root level="ALL">
			<AppenderRef ref="Console" level="INFO" />
		</Root>
	</Loggers>
</Configuration>